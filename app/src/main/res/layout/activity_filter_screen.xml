<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="filterScreenVM" type="com.bloom.app.modules.filterscreen.data.viewmodel.FilterScreenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameFilterScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_900">
        <ImageView android:id="@+id/imageMap"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_map}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_map"/>
        <LinearLayout android:id="@+id/linearColumngroup5322"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStylegray_901">
                <LinearLayout android:id="@+id/linearColumntime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearIPhoneSimgeler"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageTime"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_29pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_time"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_time"/>
                        <LinearLayout
                            android:id="@+id/linearRowcellularconnec"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_236pxh"
                            android:layout_weight="0.69"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageCellularConnec"
                                android:layout_height="@dimen/_10pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_17pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_cellularconnec"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_cellularconnec"/>
                            <ImageView android:id="@+id/imageWifi"
                                android:layout_height="@dimen/_10pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_width="@dimen/_15pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_wifi"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_wifi"/>
                            <ImageView android:id="@+id/imageBattery"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_battery"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_battery"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowgroup5320"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_38pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageGroup5320"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group5320"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group5320"/>
                        <FrameLayout
                            android:id="@+id/frameStackbusinesswheel"
                            android:layout_height="@dimen/_31pxv"
                            android:layout_marginStart="@dimen/_64pxh" android:layout_width="@dimen/_193pxh">
                            <TextView android:ellipsize="end"
                                android:gravity="top|start"
                                android:id="@+id/txtBusinessWheel"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{filterScreenVM.filterScreenModel.txtBusinessWheel}"
                                style="@style/txtInterextrabold25" tools:text="@string/lbl_businesswheel"/>
                            <TextView android:ellipsize="end"
                                android:gravity="top|start"
                                android:id="@+id/txtBusinessWheelOne"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{filterScreenVM.filterScreenModel.txtBusinessWheelOne}"
                                style="@style/txtInterextrabold25" tools:text="@string/lbl_businesswheel"/>
                        </FrameLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageGroup5322"
                android:layout_gravity="end"
                android:layout_height="@dimen/_44pxh"
                android:layout_marginEnd="@dimen/_122pxh"
                android:layout_marginStart="@dimen/_122pxh"
                android:layout_marginTop="@dimen/_122pxv"
                android:layout_width="@dimen/_33pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group5322"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group5322"/>
            <FrameLayout android:id="@+id/frameStackellipsesix"
                android:layout_height="@dimen/_471pxv"
                android:layout_marginTop="@dimen/_69pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearIconLocation"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_162pxh"
                    android:layout_marginStart="@dimen/_162pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_1">
                    <View
                        android:background="@drawable/rectangle_bg_gray_900_radius_5_1"
                        android:id="@+id/viewEllipseSix"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_10pxh"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageGroup5322One"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group5322"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group5322"/>
                <FrameLayout android:id="@+id/frameStackgroup5322Two"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_419pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageGroup5322Two"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_56pxh"
                        android:layout_marginStart="@dimen/_56pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group5322"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group5322"/>
                    <LinearLayout android:id="@+id/linearBarFilter"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylegray_901cornerRadius">
                        <View
                            android:background="@drawable/rectangle_bg_gray_100_radius_1_5"
                            android:id="@+id/lineRectangle385"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_3pxv"
                            android:layout_marginEnd="@dimen/_23pxh"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_50pxh"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnbatterylevel"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_18pxh"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_46pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtBatteryLevel"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{filterScreenVM.filterScreenModel.txtBatteryLevel}"
                                style="@style/txtInterregular15" tools:text="@string/lbl_battery_level"/>

                            <com.google.android.material.slider.RangeSlider
                                android:gravity="center_horizontal"
                                android:id="@+id/rangeBarGroupTwentyNine"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_width="match_parent"
                                android:valueFrom="0"
                                android:valueTo="100"
                                app:labelBehavior="gone"
                                app:thumbColor="@color/light_blue_A700"
                                app:thumbRadius="@dimen/_8pxh"
                                app:trackColorActive="@color/teal_900"
                                app:trackColorInactive="@color/gray_800" app:values="@array/range_slider_values"/>
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearRowthirtyfive"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtThirtyFive"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.4"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{filterScreenVM.filterScreenModel.txtThirtyFive}"
                                    style="@style/txtABeeZeeregular13_1" tools:text="@string/lbl_35"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtOneHundred"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_169pxh"
                                    android:layout_weight="0.6"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{filterScreenVM.filterScreenModel.txtOneHundred}"
                                    style="@style/txtABeeZeeregular13_1" tools:text="@string/lbl_100"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnscooterdistanc"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_23pxh"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_28pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtScooterDistanc"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{filterScreenVM.filterScreenModel.txtScooterDistanc}"
                                style="@style/txtInterregular15" tools:text="@string/msg_scooter_distanc"/>

                            <com.google.android.material.slider.RangeSlider
                                android:gravity="center_horizontal"
                                android:id="@+id/rangeBarGroupThirtyOne"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_width="match_parent"
                                android:valueFrom="0"
                                android:valueTo="100"
                                app:labelBehavior="gone"
                                app:thumbColor="@color/light_blue_A700"
                                app:thumbRadius="@dimen/_8pxh"
                                app:trackColorActive="@color/teal_900"
                                app:trackColorInactive="@color/gray_800" app:values="@array/range_slider_values1"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearRowdistance"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDistance"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.36"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{filterScreenVM.filterScreenModel.txtDistance}"
                                    style="@style/txtABeeZeeregular13_1" tools:text="@string/lbl_0m"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDistanceOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_227pxh"
                                    android:layout_weight="0.64"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{filterScreenVM.filterScreenModel.txtDistanceOne}"
                                    style="@style/txtABeeZeeregular13_1" tools:text="@string/lbl_750m"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnshowWorkScooters"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_58pxv"
                            android:layout_marginEnd="@dimen/_23pxh"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_31pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius">
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnShowWorkScooters"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_12pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_12pxv"
                                android:singleLine="true"
                                android:text="@string/msg_show_work_scoot"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_1" tools:text="@string/msg_show_work_scoot"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
